// A launch configuration that launches the extension inside a new window
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension - Workspace",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "${workspaceFolder}/run_env/sample-oe-projects/sample.code-workspace"
      ],
      "sourceMaps": true,
      "preLaunchTask": "build"
    },
